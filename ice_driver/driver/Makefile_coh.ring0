ifneq ($(KERNELRELEASE),)

include $(src)/Kbuild

else

KDIR ?= /lib/modules/`uname -r`/build

default:
	$(MAKE) -C $(KDIR) M=$$PWD COH=1

clean:
	$(MAKE) -C $(KDIR) M=$$PWD COH=1 clean

endif
